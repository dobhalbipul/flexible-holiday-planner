PORT=5000
NODE_ENV=development
# Database Configuration (using example values)
DATABASE_URL=postgresql://username:password@hostname:5432/database_name

# Stripe Configuration (replace with your actual keys)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Frontend Stripe Configuration (accessible in client-side code)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_publishable_key_here

# Razer Pay Configuration (replace with your actual keys)
RAZER_SECRET_KEY=your_razer_secret_key_here
RAZER_PUBLIC_KEY=your_razer_public_key_here

# Frontend Razer Pay Configuration (accessible in client-side code)
VITE_RAZER_PUBLIC_KEY=your_razer_public_key_here

# Amadeus API Configuration (replace with your actual keys)
AMADEUS_CLIENT_ID=your_amadeus_client_id_here
AMADEUS_CLIENT_SECRET=your_amadeus_client_secret_here
